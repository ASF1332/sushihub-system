{
  "name": "sushihub",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:backend": "ts-node-dev src/backend/index.ts",
    "dev:frontend": "npm run dev --prefix src/frontend",
    "dev": "concurrently --kill-others-on-fail \"npm:dev:*\"",
    "build:frontend": "npm run build --prefix src/frontend",
    "build:backend": "tsc --project tsconfig.backend.json",
    "build": "npm run build:frontend && npm run build:backend",
    "start": "node dist/backend/index.js",
    "prisma:generate": "node node_modules/prisma/build/index.js generate --schema=src/backend/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "prisma": "^5.22.0",
    "recharts": "^3.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.25",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "prisma": {
    "schema": "src/backend/prisma/schema.prisma",
    "seed": "npx tsx src/backend/seed.ts"
  }
}
